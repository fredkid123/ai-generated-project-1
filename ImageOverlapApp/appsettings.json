{
	"SimilarityThreshold": 0.85,
	"Serilog": {
		"Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
		"MinimumLevel": "Information",
		"WriteTo": [
			{ "Name": "Console" },
			{
				"Name": "File",
				"Args": {
					"path": "/log/ImageOverlapApp/log-.txt",
					"rollingInterval": "Day",
					"rollOnFileSizeLimit": true,
					"fileSizeLimitBytes": 10485760
				}
			}
		]
	}
}